  "private": true,
  "scripts": {
    "start": "ts-node main",
    "dev": "ts-node-dev main",
    "dev:autolint": "nodemon main --exec 'npm run lint && node'",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --runInBand --detectOpenHandles",
