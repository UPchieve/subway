{
  "name": "upchieve-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:frontend && npm run build:backend",
    "lint": "npm run lint:backend:dry && npm run lint:frontend:dry",
    "build:backend": "./bin/build_backend.sh",
    "build:frontend": "vue-cli-service build",
    "dev": "npm run build:frontend && npm run dev:backend",
    "dev:backend": "ts-node-dev --watch server,src server/main",
    "dev:frontend": "vue-cli-service build --mode=production --watch",
    "dev:init": "ts-node server/init.ts",
    "html-validate": "html-validate --ext html,vue src",
    "lint:backend": "prettier --write \"server/**/*.ts\"",
    "lint:backend:dry": "prettier --check \"server/**/*.ts\"",
    "lint:frontend": "eslint ./src -c .eslintrc-vue.js --ignore-path .eslintignore-vue --fix --ext .js,.vue",
    "lint:frontend:dry": "eslint ./src -c .eslintrc-vue.js --ignore-path .eslintignore-vue --no-fix --max-warnings 0 --ext .js,.vue",
    "lint:write": "npm run lint:backend:write && npm run lint:frontend:write",
    "start": "node build/main",
    "start-local": "ts-node server/init.ts && node build/main",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest -c jest.config.server.js --silent --group=-email --max_old_space_size=1536 --maxWorkers=3",
    "test:backend:debug": "jest -c jest.config.server.js --logHeapUsage --detectOpenHandles --group=-email",
    "test:backend:full": "jest -c jest.config.server.js --detectOpenHandles --silent",
    "test:backend:watch": "jest -c jest.config.server.js --watch --runInBand --detectOpenHandles",
    "test:frontend": "vue-cli-service test:unit -c jest.config.vue.js",
    "worker": "node build/worker",
    "worker-local": "ts-node server/init.ts && node build/worker",
    "worker:dev": "ts-node-dev server/worker",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
